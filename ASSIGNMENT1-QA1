#include <iostream>
using namespace std;

bool possible(int arr[], int n, int m, int x){
    int segments = 1;
    int curr = 0;

    for(int i=0;i<n;i++){
        if((curr | arr[i]) <= x)
            curr |= arr[i];
        else{
            segments++;
            curr = arr[i];
        }
    }
    return segments <= m;
}

int main(){
    int arr[] = {1, 2, 3, 4};
    int n = 4;
    int m = 2;

    int low = 0, high = 0;
    for(int i=0;i<n;i++) high |= arr[i];

    int ans = high;
    while(low <= high){
        int mid = (low + high) / 2;
        if(possible(arr, n, m, mid)){
            ans = mid;
            high = mid - 1;
        }else{
            low = mid + 1;
        }
    }

    cout << ans << endl;
    return 0;
}
