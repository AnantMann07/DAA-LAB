#include <iostream>
using namespace std;

int main(){
    int arr[] = {1, 3, 7, 12, 13};
    int n = 5;

    for(int i=0;i<n-1;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i] > arr[j]){
                int t = arr[i];
                arr[i] = arr[j];
                arr[j] = t;
            }
        }
    }

    int ans = 1;

    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            if(arr[j] - arr[i] <= 10){
                if(j - i + 1 > ans)
                    ans = j - i + 1;
            }
        }
    }

    cout << ans << endl;
    return 0;
}
